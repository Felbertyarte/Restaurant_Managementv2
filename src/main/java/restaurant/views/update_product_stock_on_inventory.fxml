<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<DialogPane prefHeight="271.0" prefWidth="513.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="restaurant.controllers.update_product_stock_on_inventory_controller">
   <content>
      <AnchorPane prefHeight="227.0" prefWidth="513.0">
         <children>
            <VBox alignment="TOP_CENTER" layoutX="10.0" layoutY="7.0" prefHeight="200.0" prefWidth="483.0">
               <children>
                  <AnchorPane prefHeight="27.0" prefWidth="483.0" />
                  <Label fx:id="product_name" alignment="TOP_LEFT" text="Product Name: Bun">
                     <font>
                        <Font name="System Bold Italic" size="14.0" />
                     </font>
                  </Label>
                  <AnchorPane prefHeight="33.0" prefWidth="483.0" />
                  <Label fx:id="recent_stock" text="Recent Stock: 100">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <AnchorPane prefHeight="14.0" prefWidth="483.0" />
                  <TextField fx:id="updated_stock_tf" prefHeight="25.0" prefWidth="509.0" promptText="Input Updated Stock" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="APPLY" />
      <ButtonType fx:constant="CANCEL" />
   </buttonTypes>
</DialogPane>
